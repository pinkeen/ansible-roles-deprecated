#!/bin/expect -f

set console [lindex $argv 0]
set email [lindex $argv 1]
set name [lindex $argv 2]
set pass [lindex $argv 3]

spawn "$console" phpci:create-admin

expect "Admin Email:"
send "$email\n"

expect "Admin Name:"
send "$name\n"

expect "Admin Password:"
send "$pass\n"

expect "User account created!"
